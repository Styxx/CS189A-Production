<% current_user.genres.each do |g| %>
    <%= g.genre %><br>
<% end %>
<% current_user.instruments.each do |inst| %>
    <%= inst.instrument + " " + inst.play.to_s %><br>
<% end %>
<br><br>
<%= @users %>
<% @users.each do |id, score| %>
    <% user = User.find(id.to_i) %>
    <%= user.name  + " " + score.to_s + " " + current_user.id.to_s + " " + user.id.to_s%><br>
    <% user.genres.each do |g| %>
        <%= g.genre %><br>
        <% end %>
  <% user.instruments.each do |inst| %>
        <%= inst.instrument + " " + inst.play.to_s + " " + inst.experience.to_s%> <br>
    <% end %><br>
<% end %>