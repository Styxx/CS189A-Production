<div class="container">
  <div class="row">
    <div class="[ col-xs-12 col-sm-offset-2 col-sm-8 ]">
      <ul class="event-list">
        <% @events_title.zip @events_descript,@events_date,@events_month,@events_venue,@events_url do |event| %>
            <li>
              <time datetime="2014-07-20">
                <span class="day"><%= event[2] %></span>
                <span class="month"><%= event[3] %></span>
              </time>
              <img alt="Independence Day" src="https://farm4.staticflickr.com/3100/2693171833_3545fb852c_q.jpg" />
              <div class="info">
                <h2 class="title"><%= event[0] %></h2>
                <p class="desc"><%= event[1] %></p>
              </div>
            </li>
        <% end %>
      </ul>

      <h3>User Events</h3>

      <ul class="event-list">
        <% @user_title.zip @user_descript,@user_date,@user_month,@user_venue,@user_url do |event| %>
            <li>
              <time datetime="2014-07-20">
                <span class="day"><%= event[2] %></span>
                <span class="month"><%= event[3] %></span>
              </time>
              <img alt="Independence Day" src="https://farm4.staticflickr.com/3100/2693171833_3545fb852c_q.jpg" />
              <div class="info">
                <h2 class="title"><%= event[0] %></h2>
                <p class="desc"><%= event[1] %></p>
              </div>
            </li>
        <% end %>
      </ul>
    </div>
    <div>
      <%= link_to add_event_path, id: "icon" do %>
          <span class="fa fa-plus-circle  fa-2x" align="center"><div class="osw"><font size="4">Add your own Event!</font></div></span>
      <% end %>
  </div>
  </div>
</div>
